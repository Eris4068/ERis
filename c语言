#include<stdio.h>
void f(int x){
	int i,n;
	for (n=2;n<=x;n++){
		if ((x%n==0)&&(n!=x)){
		i = 1;
		break;
		}else{
		i = 0;
		}
	}
	if (i==0){
	printf(" %d",x);
	}
}
int main(){
    int n,x;
	for(x=2;x<=1000;x++){
	   f(x);
（4）
#include<stdio.h>
void f(int x){
   int a,b,c;
   a = x/100;
   b = (x-a*100)/10;
   c = x-a*100-b*10;
	   if (a*a*a+b*b*b+c*c*c==x){
	   printf(" %d",x);
	   
	   }

}
int main(){
int n = 100;
for (n=100;n<=999;n++){
     f(n);
}

}
