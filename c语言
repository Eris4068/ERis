#include<stdio.h>
int f(int x,int y,int z){
int b;
if (x>y){
   b = x;
}else {b=y;}
if (z>=b){
	b =z;
}
return b;
}

int main(){
    int x,y,z;
	printf("输入三个数\(并用空格隔开\)：");
    scanf("%d%d%d",&x,&y,&z);
	printf("%d",f(x,y,z));
}
